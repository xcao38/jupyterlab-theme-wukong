    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/rendermime": "^2.2.4",
    "@jupyterlab/rendermime-interfaces": "^2.2.1"
  },
  "devDependencies": {
    "rimraf": "~3.0.0",
    "typedoc": "^0.15.4",
    "typescript": "~3.7.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "mimeExtension": true
  },
  "gitHead": "51fe0db3c6b91322d21f3a7cd8621f9abc4515e2"
